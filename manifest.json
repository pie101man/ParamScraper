{
  "manifest_version": 3,
  "name": "Parameter Miner Helper",
  "version": "1.0",
  "description": "Collects parameter names for use with Param Miner",
  "background": {
    "service_worker": "background.js"
  },
    "action": {
        "default_popup": "popup.html"
      },
  "permissions": [
    "webRequest",
    "storage",
    "activeTab"
  ],
  "host_permissions": [
    "<all_urls>"
  ]
}